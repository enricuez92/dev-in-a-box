{
  "version": "5.0.0-beta.22",
  "network": "regtest",
  "port": 3001,
  "services": [
    "insight-ui",
    "insight-api",
    "block",
    "db",
    "fee",
    "header",
    "address",
    "mempool",
    "p2p",
    "timestamp",
    "transaction",
